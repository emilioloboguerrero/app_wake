{
  "name": "wake-pwa",
  "version": "1.1.10",
  "description": "Wake - Consumer PWA (Expo / React Native)",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel --clear",
    "start:dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "export:web": "expo export --platform web",
    "build:android": "expo build:android",
    "build:ios": "npx eas-cli build --platform ios --profile production",
    "build:ios:local": "npx eas-cli build --local --platform ios --profile production",
    "build:dev:ios": "npx eas-cli build --platform ios --profile development",
    "build:dev:android": "npx eas-cli build --platform android --profile development",
    "build:dev:ios:local": "npx eas-cli build --local --platform ios --profile development",
    "build:dev:android:local": "npx eas-cli build --local --platform android --profile development",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint ."
  },
